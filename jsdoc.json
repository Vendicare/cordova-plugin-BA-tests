{	
	"source": {
        "include": ["www"]
    },  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./docs/gen",
    "readme": "README.JSDOC.md",
    "lenient": false,
    "sort": false
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "path": "ink-docstrap",
    "theme": "cerulean",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
